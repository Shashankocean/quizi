                        <Grid Margin="0,303,36,0" Height="116" VerticalAlignment="Top">
                            <Grid x:Name="Grid_C" Margin="10,10,430,0" Height="85" VerticalAlignment="Top">
                                <Grid.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.4">
